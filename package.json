{
  "name": "white-label",
  "version": "1.0.0",
  "description": "A Vinyl-Trading enterprise app built with Node.js + TypeScript using Domain-Driven Design",
  "main": "bin/www",
  "dependencies": {
    "chai": "^4.2.0",
    "dotenv": "^8.0.0",
    "mysql2": "^1.6.5",
    "nodemon": "^1.19.1",
    "sequelize": "^5.8.12",
    "shallow-equal-object": "^1.1.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "sqlite3": "^4.0.9",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "dotenv-cli": "^2.0.0",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "sequelize-cli": "^5.5.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "db:create": "node scripts/db/create",
    "db:delete": "node scripts/db/delete",
    "db:flush": "npm run db:delete && npm run db:create",
    "migrate:dev": "npm run build && dotenv -e .env sequelize db:migrate",
    "migrate:prod": "npm run build && dotenv -e .env sequelize db:migrate --env production",
    "start:dev": "nodemon",
    "test": "jest",
    "test:dev": "jest --watch",
    "testWithCoverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stemmlerjs/white-label.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "domain-driven-design"
  ],
  "author": "Khalil Stemmler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stemmlerjs/white-label/issues"
  },
  "homepage": "https://github.com/stemmlerjs/white-label#readme"
}
